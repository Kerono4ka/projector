// cheack if we have eventListener at the ajax and remove them then add new one 
$("#add-kanban").off('ajax:success').on('ajax:success', function() {
  $.ajax({
    url: <%= @board.id %>+"/columns",
    method: "POST",
    headers: { 'X-CSRF-Token': document.getElementsByName( "csrf-token" )[0].content }
  });
});